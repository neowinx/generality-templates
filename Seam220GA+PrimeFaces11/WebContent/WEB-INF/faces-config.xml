<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="1.2" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xi="http://www.w3.org/2001/XInclude"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
 <managed-bean>
  <managed-bean-name>beanFormCab</managed-bean-name>
  <managed-bean-class>com.knowhow.recurspe.session.BeanFormCab</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>beanPeriodo</managed-bean-name>
  <managed-bean-class>com.knowhow.recurspe.session.BeanPeriodo</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>beanEstado</managed-bean-name>
  <managed-bean-class>com.knowhow.recurspe.session.BeanEstado</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>beanTipForm</managed-bean-name>
  <managed-bean-class>com.knowhow.recurspe.session.BeanTipForm</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>simpleTreeBean</managed-bean-name>
  <managed-bean-class>com.knowhow.recurspe.arbol.SimpleTreeBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>documentsController</managed-bean-name>
  <managed-bean-class>com.knowhow.recurspe.test.DocumentsController</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>accountTreeTableController</managed-bean-name>
  <managed-bean-class>com.knowhow.recurspe.arbol.AccountTreeTableController</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>beanUsuario</managed-bean-name>
  <managed-bean-class>com.knowhow.recurspe.session.BeanUsuario</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>cuentasSeleccionController</managed-bean-name>
  <managed-bean-class>com.knowhow.recurspe.arbol.CuentasSeleccionController</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>beanFormula</managed-bean-name>
  <managed-bean-class>com.knowhow.recurspe.session.BeanFormula</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <navigation-rule>
  <display-name>BalanceGral</display-name>
  <navigation-case>
   <from-outcome>crearFormCab</from-outcome>
   <to-view-id>/CargaFormularios/CreaFormularioCabecera.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>modificarFormCab</from-outcome>
   <to-view-id>/ModificacionFormularios/ModificaFormularioCabecera.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <navigation-case>
   <from-outcome>home</from-outcome>
   <to-view-id>/index.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <description>Direcciona a pantalla Agregar Formula</description>
  <display-name>agregarFormula</display-name>
  <navigation-case>
   <from-outcome>agregarFormula</from-outcome>
   <to-view-id>/CargaFormularios/CreaFormula.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>success</display-name>
  <from-view-id>/ListadoFormularios/ListadoTiposDeFormularios.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>Realizado</from-outcome>
   <to-view-id>/ListadoFormularios/ListadoTiposDeFormularios.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>mostrarArbolCuentasAsociadas</display-name>
  <from-view-id>/ListadoFormularios/ListadoTiposDeFormularios.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>mostrarArbolCuentasAsociadas</from-outcome>
   <to-view-id>/ModificacionFormularios/arbolDeCuentasSeleccion.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>volver</display-name>
  <from-view-id>/ModificacionFormularios/arbolDeCuentasSeleccion.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>volver</from-outcome>
   <to-view-id>/ListadoFormularios/ListadoTiposDeFormularios.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>Realizado</from-outcome>
   <to-view-id>/ListadoFormularios/ListadoTiposDeFormularios.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>Error</from-outcome>
   <to-view-id>/ModificacionFormularios/arbolDeCuentasSeleccion.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <application>
  <view-handler>com.sun.facelets.FaceletViewHandler</view-handler>
 </application>
</faces-config>
